# 初始化水平集函数 phi
phi = initialize_level_set()

for iteration in range(max_iterations):
    # 计算梯度
    grad_phi_x, grad_phi_y = compute_gradient(phi)
    |grad_phi| = np.sqrt(grad_phi_x**2 + grad_phi_y**2)
    
    # 计算曲率 kappa
    kappa = compute_curvature(phi)
    
    # 计算膨胀力和正则化项
    data_term = c * |grad_phi|
    reg_term = mu * delta_epsilon(phi) * kappa
    
    # 更新水平集函数
    dphi_dt = data_term + reg_term
    phi += dt * dphi_dt
    
    # 重新初始化
    if iteration % reinit_interval == 0:
        phi = reinitialize(phi)